@{
    ViewBag.Title = "TCPSpike";
}





@using (Html.BeginForm())
{
    @model Destroy_Azure.BundleConfig
    @Html.ActionLink("linkText", "Action", new { controller = "Controller" }, new { target = "~/PrintConnectionsSummary.aspx" })
    <a href="@Url.Action("Action", "Controller")" target="~/PrintConnectionsSummary.aspx">Link Text</a>

    <script>
        window.open("~/PrintConnectionsSummary.aspx");
    </script>

    @Html.AntiForgeryToken()

    <div class="form-horizontal">
        <h4>BundleConfig</h4>
        <hr />
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Create" class="btn btn-default" />
            </div>
        </div>
    </div>
}

<div>
    @Html.ActionLink("Back to List", "Index")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
